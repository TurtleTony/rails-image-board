<h3>Dashboard</h3>
<% if user_signed_in? %>
  <%= link_to "Upload", new_post_path %>
  <%= link_to "Log out", destroy_user_session_path, method: :delete %>
<% end %>

<br>

<% @posts.each do |post| %>
  <%= image_tag(url_for(post.resource)) %>
  <%= link_to "Delete", post_path(post), method: :delete, data: { confirm: "Are you sure?" } %>
  <br>
<% end %>